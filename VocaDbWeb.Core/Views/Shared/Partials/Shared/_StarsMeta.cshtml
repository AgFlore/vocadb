@using VocaDb.Web.Models.Shared.Partials.Shared
@model StarsMetaViewModel

@for (int i = 1; i <= Model.Max; ++i) {
	if (Model.Current >= i) {
		<img class="rating" src="@Url.Content("~/Content/star.png")" alt="@Model.Current" />
	} else {
		<img class="rating" src="@Url.Content("~/Content/star_disabled.png")" alt="@Model.Current" />
	}
}