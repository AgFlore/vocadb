@using Microsoft.AspNetCore.Mvc.Rendering
@using VocaDb.Web.Code
@using VocaDb.Web.Models.Shared.Partials.ArchivedEntry
@model VocaDb.Web.Models.Artist.Versions

@{
	PageProperties.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Artist.Name;
	PageProperties.Robots = PagePropertiesData.Robots_Noindex_Nofollow;
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Artists, "Index"), 
		Html.ActionLink(Model.Artist.Name, "Details", new { id = Model.Artist.Id }) 
	};
}

@Html.Partial("Partials/ArchivedEntry/_CurrentVersionMessage", new CurrentVersionMessageViewModel(Model.Artist.Version, Model.Artist.Status))

@Html.Partial("Partials/ArchivedEntry/_ArchivedObjectVersions", new ArchivedObjectVersionsViewModel(Model.ArchivedVersions, id => Url.Action("ViewVersion", new { id = id })))