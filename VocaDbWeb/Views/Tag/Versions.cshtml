@using VocaDb.Model.Domain.Tags
@using VocaDb.Web.Helpers
@using VocaDb.Web.Models.Shared
@inherits VocaDb.Web.Code.VocaDbPage<VocaDb.Model.DataContracts.Tags.TagWithArchivedVersionsContract>

@{
	PageProperties.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Name;
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Tags, "Index"), 
		Html.ActionLink(Model.Name, "DetailsById", new { id = Model.Id, slug = Model.UrlSlug }) 
	};
}

@ArchivedEntryHelpers.CurrentVersionMessage(Model.Version, Model.Status)

@ArchivedEntryHelpers.ArchivedObjectVersions(Model.ArchivedVersions.Select(
	a => new ArchivedObjectVersion(a, Translate.TagEditableFieldNames.GetAllNameNames(a.ChangedFields, TagEditableFields.Nothing))))