@inherits VocaDb.Web.Code.VocaDbPage<VocaDb.Web.Models.Tag.Versions>

@{
	PageProperties.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Entry.Name;
	PageProperties.Robots = "noindex,nofollow";
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Tags, "Index"), 
		Html.ActionLink(Model.Entry.Name, "DetailsById", new { id = Model.Entry.Id, slug = Model.Entry.UrlSlug }) 
	};
}

@ArchivedEntryHelpers.CurrentVersionMessage(Model.Entry.Version, Model.Entry.Status)

@ArchivedEntryHelpers.ArchivedObjectVersions(Model.ArchivedVersions, id => Url.Action("ViewVersion", new { id }))