@using VocaDb.Web.Models.Shared.Partials.ArchivedEntry;
@inherits VocaDb.Web.Code.VocaDbPage<VocaDb.Web.Models.Song.Versions>

@{
	PageProperties.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Song.Name;
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Songs, "Index", "Search", UrlMapper.Search.Songs(), null), 
		LinkExtensions.ActionLink(Html, Model.Song.Name, "Details", new { id = Model.Song.Id }) 
	};
}

@{ Html.RenderPartial("Partials/ArchivedEntry/_CurrentVersionMessage", new CurrentVersionMessageViewModel(Model.Song.Version, Model.Song.Status)); }

@{ Html.RenderPartial("Partials/ArchivedEntry/_ArchivedObjectVersions", new ArchivedObjectVersionsViewModel(Model.ArchivedVersions, id => Url.Action("ViewVersion", new { id = id }))); }