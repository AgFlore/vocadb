@using VocaDb.Web.Models.Shared.Partials.ArchivedEntry;
@inherits VocaDb.Web.Code.VocaDbPage<VocaDb.Web.Models.Shared.Versions<VocaDb.Model.DataContracts.ReleaseEvents.ReleaseEventSeriesContract>>

@{
	PageProperties.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Entry.Name;
	ViewBag.Parents = new[] {
		Html.ActionLink(ViewRes.SharedStrings.ReleaseEvents, "Index"),
		Html.ActionLink(Model.Entry.Name, "SeriesDetails", new { id = Model.Entry.Id })
	};
}

@ArchivedEntryHelpers.CurrentVersionMessage(Model.Entry.Version, Model.Entry.Status)

@{ Html.RenderPartial("Partials/ArchivedEntry/_ArchivedObjectVersions", new ArchivedObjectVersionsViewModel(Model.ArchivedVersions, id => Url.Action("ViewSeriesVersion", new { id = id }))); }