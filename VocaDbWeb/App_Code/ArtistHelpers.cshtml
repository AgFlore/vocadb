@using System.Web.Mvc;
@using System.Web.Mvc.Html;
@using VocaDb.Model.DataContracts.Artists;
@using VocaDb.Web.Helpers;
@using VocaDb.Web.Models.Shared.Partials.Artist;
@inherits VocaDb.Web.Code.HelperPage

@helper ArtistLinkList(IEnumerable<ArtistContract> artists, bool typeLabel = false, bool releaseYear = false) {
	@Html.LinkList(artists, a => new MvcHtmlString(Html.Partial("Partials/Artist/_ArtistLink", new ArtistLinkViewModel(a, typeLabel, releaseYear: releaseYear)).ToString()))
}